    <select id="selectServerCompanyWithUsersAndMembers" resultMap="ServerCompanyWithStatsMap">
        SELECT
        c.id as company_id,
        ANY_VALUE(c.name) AS name,
        ANY_VALUE(c.info) AS info,
        ANY_VALUE(c.address) AS address,
        ANY_VALUE(c.gps) AS gps,
        ANY_VALUE(c.scale) AS scale,
        ANY_VALUE(c.createdate) AS createdate,
        ANY_VALUE(c.joindate) AS joindate,
        ANY_VALUE(c.flag) AS flag,
        ANY_VALUE(c.type) AS type,
        ANY_VALUE(c.linkman) AS linkman,
        ANY_VALUE(c.phone) AS phone,
        COUNT(DISTINCT u.id) AS user_count,
        ANY_VALUE(m.level) AS member_level
        FROM
        t_server_commpany c
        LEFT JOIN t_user u ON u.companyid = c.id
        LEFT JOIN t_member_commpany mc
        ON mc.commpanyid = c.id
        AND #{currentDate} BETWEEN mc.startdate AND mc.enddate
        LEFT JOIN t_member m ON mc.memberid = m.id
        <where>
            <if test="id != null">
                AND c.id = #{id}
            </if>
            <if test="name != null and name != ''">
                AND c.name LIKE CONCAT('%', #{name}, '%')
            </if>
            <if test="phone != null and phone != ''">
                AND c.phone = #{phone}
            </if>
            <if test="level != null and level !=''">
                AND m.level = #{level} -- 如果要根据会员等级筛选
            </if>
            <if test="flag != null">
                AND c.flag = #{flag}
            </if>
            <if test="type != null">
                AND c.type = #{type}
            </if>
        </where>
        GROUP BY c.id
    </select>

